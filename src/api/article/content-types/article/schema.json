{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true,
    "populateCreatorFields": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Tag": {
      "type": "enumeration",
      "enum": [
        "Steeple Chase",
        "Sprint",
        "Parcours"
      ]
    },
    "Content": {
      "type": "richtext"
    },
    "Banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Description": {
      "type": "text"
    },
    "Tweet": {
      "type": "customField",
      "customField": "plugin::oembed.oembed"
    }
  }
}
